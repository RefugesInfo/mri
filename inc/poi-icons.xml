<Layer name="amenity">
  <StyleName>points_importance_4</StyleName>
  <StyleName>points_importance_3</StyleName>
  <StyleName>points_importance_2</StyleName>
  <StyleName>points_importance_1</StyleName>
  <Datasource>
    &postgis-settings;
    <Parameter name="table">(select * from planet_osm_point where historic IS NOT NULL or amenity IS NOT NULL or tourism is NOT NULL or railway is NOT NULL or "natural" is NOT NULL or man_made is NOT NULL or highway is NOT NULL or aeroway is NOT NULL or power_source is NOT NULL) as amenity</Parameter>
  </Datasource>
</Layer>

<Style name="power_towers">
    <Rule>
      &maxscale_zoom14;
      <PointSymbolizer file="symbols/power_tower.png" allow-overlap="false"/>
    </Rule>
</Style>
<Layer name="power_towers">
    <StyleName>power_towers</StyleName>
    <Datasource>
    &postgis-settings;
    <Parameter name="table">(select way from planet_osm_point where power='tower') as power_towers</Parameter>
  </Datasource>
</Layer>
